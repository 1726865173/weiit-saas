/* pages/template/template.wxss */

/* 商品列表 */

.wares_list {
  overflow: hidden;
}

.list_head {
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  text-align: center;
  position: relative;
}

.more {
  position: absolute;
  right: 20rpx;
  top: 0rpx;
  font-size: 32rpx;
  color: #ec534e;
}

.icon-youjiantouda {
  font-size: 30rpx;
  font-weight: bold;
}


.list_num {
  width: 48vw;
  float: left;
  height: 68vw;
  overflow: hidden;
  margin: 1vw;
  position: relative;
  background-color: #fff;
}

.Box_img {
  width: 48vw;
  height: 48vw;
}

.Box_img .list_img {
  width: 100%;
  height: 100%;
}

.list_title {
  font-size: 30rpx;
  padding: 0 20rpx;
  padding-top: 16rpx;
  height: 75rpx;
  color: #333;
  line-height: 40rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.list_money {
  font-size: 28rpx;
  padding-left: 20rpx;
  color: #ec534e;
  position: absolute;
  bottom: 12rpx;
}

.list_money text {
  font-size: 22rpx;
}

